{
  "name": "fp-json-cli",
  "version": "0.1.2",
  "description": "Simple cli inspired by jq",
  "keywords": [
    "cli",
    "fp",
    "jq",
    "json",
    "query"
  ],
  "repository": "github:maxwihlborg/fp-json-query",
  "license": "MIT",
  "type": "module",
  "bin": {
    "fq": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && esbuild --platform=node --format=esm --bundle --minify ./src/index.ts --outdir=dist",
    "build:readme": "./scripts/update-readme.js",
    "prepublish": "rimraf dist && ncc build -m -o dist ./src/index.ts && ./scripts/update-readme.js",
    "test": "vitest"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^24.0.3",
    "cac": "^6.7.14",
    "esbuild": "^0.25.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.3.3",
    "vitest": "^3.2.3"
  }
}
